{
  "name": "rotiroti/exads-challenges",
  "description": "Solution for Exads Coding Challenges",
  "keywords": [
    "exads",
    "interview",
    "coding",
    "challenges"
  ],
  "type": "library",
  "require-dev": {
    "phpunit/phpunit": "^9.1",
    "squizlabs/php_codesniffer": "^3.5",
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "authors": [
    {
      "name": "Marco Rotiroti",
      "email": "marco.rotiroti@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "ext-pdo": "*",
    "ext-mysqli": "*",
    "vlucas/phpdotenv": "^4.1",
    "phpab/phpab": "^0.1.0"
  },
  "autoload": {
    "psr-4": {
      "Rotiroti\\Exads\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "fizz-buzz": "@php fizz-buzz.php",
    "500-element-array": "@php 500-element-array.php",
    "date-calculation": "@php date-calculation.php",
    "db-mysqli-connect": "@php db-mysqli-connect.php",
    "db-mysqli-insert": "@php db-mysqli-insert.php",
    "db-pdo-connect": "@php db-pdo-connect.php",
    "db-pdo-insert": "@php db-pdo-insert.php",
    "ab-testing": "@php ab-testing.php"
  }
}
